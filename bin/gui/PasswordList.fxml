<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="334.0" prefWidth="591.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.PasswordListController">
   <children>
      <Label prefHeight="27.0" prefWidth="143.0" text="PasswordList">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
         <padding>
            <Insets left="10.0" top="5.0" />
         </padding>
      </Label>
      <Button fx:id="btnNew" mnemonicParsing="false" onAction="#onBtNewAction" text="NEW">
         <VBox.margin>
            <Insets bottom="5.0" left="8.0" />
         </VBox.margin>
      </Button>
      <TableView fx:id="tableViewSites" prefHeight="277.0" prefWidth="484.0">
        <columns>
          <TableColumn fx:id="tableColumnId" prefWidth="39.0" text="ID" />
          <TableColumn fx:id="tableColumnUserLogin" prefWidth="160.0" text="USER" />
            <TableColumn fx:id="tableColumnPassword" prefWidth="82.0" text="PASSWORD" />
            <TableColumn fx:id="tableColumnSite" prefWidth="202.0" text="SITE" />
            <TableColumn fx:id="tableColumnEdit" prefWidth="75.0" />
        </columns>
      </TableView>
   </children>
</VBox>
